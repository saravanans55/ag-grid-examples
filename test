function compareArrays(arr1, arr2, field) {
  if (arr1.length !== arr2.length) {
    return false; // Arrays have different lengths
  }

  return arr1.every((item1, index) => item1[field] === arr2[index][field]);
}

let arr1 = [];

let arr2 = [];

let result = compareArrays(arr1, arr2, 'riskCountryTypeId');
console.log(result);
