interface Filter {
  filterType: string;
  values: string[];
}

interface MyObject {
  [key: string]: Filter;
}

let obj1: MyObject = {
  new_renewed: {
    filterType: "set",
    values: ['New']
  },
  pipeline_type: {
    filterType: "set",
    values: ['IRS']
  }
};

let obj2: MyObject = {
  new_renewed: {
    filterType: "set",
    values: ['Traditional']
  }
};

function updateObject(obj1: MyObject, obj2: MyObject): MyObject {
  // Get the keys of obj2 that also exist in obj1
  Object.keys(obj2)
    .filter(key => obj1.hasOwnProperty(key))
    .forEach(key => {
      obj1[key] = obj2[key];
    });
  return obj1;
}

const updatedObj = updateObject(obj1, obj2);

console.log(updatedObj);
