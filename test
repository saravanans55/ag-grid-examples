const columnDefs = [
  {
    field: 'numericColumn',
    filter: 'agNumberColumnFilter',
    filterParams: {
      numberParser: text => {
        // Custom numberParser to restrict to 4 digits
        const parsedValue = parseFloat(text.replace(/[^0-9]/g, '')); // Remove non-numeric characters
        return parsedValue > 9999 ? 9999 : parsedValue; // Limit to 4 digits
      },
    },
  },
];
