// Custom sorting comparator function for full month names
function monthComparator(valueA: any, valueB: any, nodeA: any, nodeB: any, isInverted: boolean): number {
  const monthsOrder = [
    "January", "February", "March", "April", "May", "June", "July",
    "August", "September", "October", "November", "December"
  ];
  const indexA = monthsOrder.indexOf(valueA);
  const indexB = monthsOrder.indexOf(valueB);

  if (indexA < indexB) {
    return -1;
  } else if (indexA > indexB) {
    return 1;
  } else {
    return 0;
  }
}

// Define your column definitions with the custom comparator
columnDefs: [
  // Other columns...
  {
    headerName: 'Month Column',
    field: 'monthField', // Replace with your actual field name
    comparator: monthComparator,
  },
],
