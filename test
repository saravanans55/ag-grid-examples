let arr1 = [
  { masterTypeName: "ABC 1032" },
  { masterTypeName: "YBC 1032" },
  { masterTypeName: "RBC 1032" },
  // ... other objects
];

// Custom sorting function
function compareMasterTypeNames(a: any, b: any): number {
  const nameA = a.masterTypeName.toLowerCase();
  const nameB = b.masterTypeName.toLowerCase();

  if (nameA < nameB) {
    return -1;
  }
  if (nameA > nameB) {
    return 1;
  }
  return 0;
}

// Sort the array in alphabetic order
arr1.sort(compareMasterTypeNames);

console.log(arr1);
