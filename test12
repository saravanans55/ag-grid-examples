private calculateCellValue() {
    if (this.params.node.group) {
      // Parent row, get identical child values for this column
      const childNodes = this.params.node.childrenAfterFilter;
      const colId = this.params.column.colId;

      // Find the first child node with a value for this column
      const firstChildWithValue = childNodes.find(node => node.data[colId]);

      if (firstChildWithValue) {
        // Use the value from the first child with a value for this column
        this.cellValue = firstChildWithValue.data[colId];

        // Set the identical value for all parent nodes within the group
        childNodes.forEach(node => {
          if (node.group) {
            node.setDataValue(colId, this.cellValue);
          }
        });
      } else {
        // No child nodes with a value for this column, show a placeholder or custom value
        this.cellValue = '-';
      }
    } else {
      // Child row, get the value from the data
      this.cellValue = this.params.value;
    }
  }
