let AndOrAndFilterPayload = [
  {
    propertyId: "record_type",
    terms: ["Opportunity"],
    type: "exactMatch"
  },
  {
    propertyId: "record_type3",
    terms: ["Opportunity3"],
    type: "exactMatch"
  },
  {
    propertyId: "record_type2",
    terms: ["Opportunity2"],
    type: "exactMatch"
  },
  {
    propertyId: "record_type1",
    terms: ["Opportunity1"],
    type: "exactMatch"
  }
];

let input = {
  propertyId: "record_type",
  terms: ["Opportunity"],
  type: "exactMatch"
};

// Comparison function to check if two objects are equal
const isEqual = (obj1, obj2) => {
  return obj1.propertyId === obj2.propertyId &&
         JSON.stringify(obj1.terms) === JSON.stringify(obj2.terms) &&
         obj1.type === obj2.type;
};

// Filter the array to exclude the input object
AndOrAndFilterPayload = AndOrAndFilterPayload.filter(item => !isEqual(item, input));

console.log(AndOrAndFilterPayload);
