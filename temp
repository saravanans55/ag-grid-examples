function removeDuplicates(arr: string[]): string[] {
  return Array.from(new Set(arr.map(item => item.split(',').sort().join(','))));
}

// Example usage
const arr = ["IP", "IRS", "IP,IRS", "IP,IRS"];
const uniqueArr = removeDuplicates(arr);
console.log(uniqueArr);  // Output: [ 'IP', 'IRS', 'IP,IRS' ]
